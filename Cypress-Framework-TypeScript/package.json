{
  "name": "cypress-framework",
  "version": "0.0.0",
  "scripts": {
    "test": "cypress open",
    "cy:run": "cypress run",
    "cy:run-allure-report": "cypress run --env allure=true",
    "ex:file": "npx nyc report --reporter=lcov",
    "lint-fix": "eslint --fix ./cypress/**/*.cy.ts",
    "remove-reports": "rimraf ./cypress/reports/* && rimraf ./mochawesome-report && rimraf ./allure-results && rimraf ./allure-reports",
    "allure-report": "allure serve ./allure-results",
    "compile-mochawsome-report": "npx mochawesome-merge ./cypress/reports/.jsons/*.json -o  mochawesomeReport.json",
    "generate-mochawsome-report": "npx marge mochawesomeReport.json"
  },
  "private": true,
  "dependencies": {
    "@types/xlsx": "^0.0.36",
    "class-transformer": "^0.5.1",
    "cypress-plugin-snapshots": "^1.4.4",
    "deep-equal-in-any-order": "^1.1.19",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "xlsx": "^0.18.5",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@shelex/cypress-allure-plugin": "^2.27.0",
    "@types/cypress-image-snapshot": "^3.1.6",
    "@types/mochawesome": "^6.2.1",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "allure-commandline": "^2.13.8",
    "axe-core": "^4.4.3",
    "cypress": "^10.4.0",
    "cypress-axe": "^1.0.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-grep": "^2.12.2",
    "cypress-image-snapshot": "^4.0.1",
    "cypress-mochawesome-reporter": "^2.4.0",
    "cypress-multi-reporters": "^1.6.1",
    "cypress-wait-until": "^1.7.2",
    "eslint": "^8.5.0",
    "jsonwebtoken": "^8.5.1",
    "junit-report-merger": "^3.0.5",
    "mocha": "^9.2.2",
    "mocha-junit-reporter": "^2.0.2",
    "mochawesome": "^7.0.1",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.0.1",
    "moment": "^2.29.3",
    "npx": "^10.2.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.5",
    "write-excel-file": "^1.4.4"
  }
